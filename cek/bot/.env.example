# CEK Bot Environment Variables
# Copy this file to .env and fill in your values

# Telegram Bot Token (required)
# Get it from @BotFather on Telegram
CEK_BOT_TOKEN=your_bot_token_here

# Database path (optional, default: /data/cek_bot.db)
# CEK_DB_PATH=/data/cek_bot.db

# Font path (optional, default: /app/resources/DejaVuSans.ttf)
# CEK_FONT_PATH=/app/resources/DejaVuSans.ttf
ADMIN_IDS=123456789
