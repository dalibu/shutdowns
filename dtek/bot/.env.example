# DTEK Bot Environment Variables
# Copy this file to .env and fill in your values

# Telegram Bot Token (required)
# Get it from @BotFather on Telegram
DTEK_BOT_TOKEN=your_bot_token_here

# Database path (optional, default: /data/dtek_bot.db)
# DTEK_DB_PATH=/data/dtek_bot.db

# Font path (optional, default: /app/resources/DejaVuSans.ttf)
# DTEK_FONT_PATH=/app/resources/DejaVuSans.ttf
ADMIN_IDS=123456789
